import java.util.Scanner;

public class Main {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        int n = sc.nextInt();
        int r;

        for (int i = 1; i <= 10; i++) {

            r = i * n;
            System.out.println("tabuada: " + n + " x " + i + " = " + r);

        }

        sc.close();



    }
}
